# Ignore everything in the vendor directory, except the autoload.php file
/vendor/*
!/vendor/autoload.php

# Ignore Composer dependencies lock file if you don't want it in version control
composer.lock

# Node modules (if you're using npm/yarn for JavaScript dependencies)
/node_modules

# Logs and debug files
*.log
*.swp
*.swo
*.bak

# Ignore OS-specific files and directories
.DS_Store
Thumbs.db

# Ignore IDE and editor-specific files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore SASS/SCSS cache files (if using pre-processing)
/.sass-cache
/.cache

# Ignore any environment configuration files
.env

# Ignore build files or artifacts (e.g., if you have a build process for assets)
build/
dist/

# Ignore WordPress core files (you should never add these to your plugin repo)
/wp-admin
/wp-includes

# Ignore uploads or other user-generated content if present in the plugin
/uploads

# Ignore specific plugin settings or files generated by WordPress (adjust as needed)
*.php~
*.php.swp
